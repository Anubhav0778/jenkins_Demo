    agent any

    stages {

        stage('Build') {
            steps {
                bat 'echo ===== BUILD FROM JENKINSFILE ====='
            }
        }

        stage('Test') {
            steps {
                bat 'echo ===== TEST FROM JENKINSFILE ====='
            }
        }

        stage('Deploy') {
            steps {
                bat 'echo ===== DEPLOY FROM JENKINSFILE ====='
            }
        }

    }
}
